{
    "name": "cethyworks/google-map-display-bundle",
    "license": "MIT",
    "description": "Provides a way to display google maps from address(es) (w/ the javascript API), the most minimalist, unobtrusive way possible.",
    "type": "symfony-bundle",
    "require": {
        "php": ">=5.6.17",
        "cethyworks/content-injector-bundle": "^2.0",
        "symfony/dependency-injection": "^3.3",
        "symfony/config": "^3.3",
        "twig/twig": "^1.0||^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "symfony/http-foundation": "^3.3",
        "symfony/event-dispatcher": "^3.3",
        "symfony/http-kernel": "^3.3",
        "symfony/framework-bundle": "~2.7|~3.0|~4.0",
        "symfony/twig-bundle": "^3.3",
        "symfony/translation": "^3.3",
        "symfony/validator": "^3.3",
        "symfony/asset": "^3.3",
        "symfony/templating": "^3.3"
    },
    "authors": [
        {
            "name": "Charles-Elie",
            "email": "charles-elie@cethyworks.com"
        }
    ],
    "autoload": {
        "psr-4": { "Cethyworks\\GoogleMapDisplayBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Cethyworks\\GoogleMapDisplayBundle\\Tests\\": "tests/" }
    }
}
